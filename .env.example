# Feature flags for gradual migration from Amplify SDK
# Set to 'true' to enable new implementation instead of Amplify SDK
# Default: false (uses Amplify SDK for safety)

# Enable new S3 storage service
REACT_APP_USE_NEW_STORAGE=false

# Enable new Cognito auth service  
REACT_APP_USE_NEW_AUTH=false

# Enable new GraphQL API service
REACT_APP_USE_NEW_API=false

# AWS Configuration (if not using aws-exports.js)
# These will override aws-exports.js values
APP_AWS_REGION=us-west-2
APP_COGNITO_USER_POOL_ID=
APP_COGNITO_CLIENT_ID=
APP_COGNITO_IDENTITY_POOL_ID=
APP_S3_BUCKET_NAME=
APP_APPSYNC_ENDPOINT=
APP_APPSYNC_API_KEY=